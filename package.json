{
  "name": "ordered-dropdown",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "build": "npm run compile && npm run package",
    "build:dev": "npm run compile:dev && npm run package:dev",
    "build:preview": "npm run compile:dev && npm run package:preview",
    "compile": "webpack --mode production",
    "compile:dev": "webpack --mode development",
    "clean": "rimraf ./dist",
    "clean-all": "rimraf ./dist ./package",
    "dev-server": "npx http-server ./ --tls -c-1",
    "package": "tfx extension create --manifests azure-devops-extension.json --output-path ./package",
    "package:dev": "tfx extension create --manifests azure-devops-extension-dev.json --output-path ./package --rev-version",
    "package:preview": "tfx extension create --manifests azure-devops-extension-preview.json --output-path ./package --rev-version",
    "watch": "webpack --mode development --watch"
  },
  "dependencies": {
    "@fluentui/react": "~8.106.2",
    "@fluentui/react-icons": "~2.0.195",
    "@types/react": "~16.14.24",
    "@types/react-dom": "~16.9.14",
    "azure-devops-extension-api": "~1.158.0",
    "azure-devops-extension-sdk": "~2.0.11",
    "react": "~16.14.0",
    "react-dom": "~16.14.0"
  },
  "devDependencies": {
    "css-loader": "~6.7.1",
    "html-webpack-plugin": "~5.5.0",
    "http-server": "~14.1.1",
    "rimraf": "~3.0.2",
    "style-loader": "~3.3.1",
    "tfx-cli": "~0.11.0",
    "ts-loader": "~9.2.8",
    "typescript": "~4.6.2",
    "webpack": "~5.70.0",
    "webpack-cli": "~4.9.2"
  }
}
